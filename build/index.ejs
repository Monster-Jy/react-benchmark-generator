<!DOCTYPE html>
<html>
  <head>
    <!-- <%= htmlWebpackPlugin.options.publicPath %>js/log4j.js -->
    <link rel="shortcut icon" href="<%= htmlWebpackPlugin.options.publicPath %>newFavicon.ico"></link>
    <!-- <link rel="shortcut icon" href="%PUBLIC_URL%/newFavicon.ico" /> -->
    <link rel="stylesheet" type="text/css" data-type="ali-iconfont" href="<%= htmlWebpackPlugin.options.publicPath %>fonts/iconfont.css?k=3318892_frdr8t39lr" />
    <% if (htmlWebpackPlugin.options.ENV === 'development') { %>
      <script>
        window.HOST_URL = '<%= htmlWebpackPlugin.options.HOST_URL %>';
        window.MicroFrontEndConfig = <%= htmlWebpackPlugin.options.MicroFrontEndConfig %>;
        document.title = window.MicroFrontEndConfig?.appName
      </script>
      <%= htmlWebpackPlugin.options.cssString %>
      <%= htmlWebpackPlugin.options.jsString %>
    <% } else { %>
      <script>
        window.HOST_URL = `${window.location.origin}/static/`;
        window.MicroFrontEndConfig = {* microFrontEndConfig *};
        document.title = window.MicroFrontEndConfig?.appName
      </script>
      {* cssString *}
      {* jsString *}
    <% } %>
  </head>
  <body>
    <div id="root"></div>
    <script>
      window.MCLog4j_Level = 1;
    </script>
    <script src="<%= htmlWebpackPlugin.options.publicPath %>js/log4j.js"></script>
  </body>
</html>
